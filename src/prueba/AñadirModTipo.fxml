<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="prueba.AñadirModTipoController">
    <stylesheets>
        <URL value="@añadirmodtipo.css" />
    </stylesheets>
   <children>
      <TableView id="tablatipo" fx:id="tablatipo" layoutX="14.0" layoutY="27.0" prefHeight="326.0" prefWidth="249.0">
        <columns>
          <TableColumn id="Idtiporow" fx:id="Idtiporow" prefWidth="75.0" text="Idtipo" />
          <TableColumn id="nombrerow" fx:id="nombrerow" prefWidth="172.0" text="Nombre" />
        </columns>
      </TableView>
      <TextField id="nombrefield" fx:id="nombrefield" layoutX="356.0" layoutY="97.0" promptText="Nombre Tipo" />
      <Label layoutX="356.0" layoutY="70.0" prefHeight="17.0" prefWidth="149.0" text="Nombre" textFill="#fffb00">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="356.0" layoutY="14.0" prefHeight="17.0" prefWidth="149.0" text="Idtipo" textFill="#fffb00">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField id="idtipofield" fx:id="idtipofield" editable="false" layoutX="356.0" layoutY="41.0" promptText="Id Tipo" />
      <Button id="modificar" fx:id="modificar" layoutX="373.0" layoutY="231.0" mnemonicParsing="false" onAction="#modificartable" prefHeight="48.0" prefWidth="115.0" text="Modificar" textFill="#148318">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button id="eliminartipo" fx:id="eliminartipo" layoutX="373.0" layoutY="305.0" mnemonicParsing="false" onAction="#eliminartipo" text="Eliminar" textFill="RED">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
      <Button id="añadirboton" fx:id="añadirboton" layoutX="373.0" layoutY="152.0" mnemonicParsing="false" onAction="#invisible" prefHeight="48.0" prefWidth="115.0" text="Añadir" textFill="#148318">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button id="aceptarboton" fx:id="aceptarboton" layoutX="263.0" layoutY="152.0" mnemonicParsing="false" onAction="#añadir" prefHeight="48.0" prefWidth="155.0" text="Aceptar Añadir" textFill="#148318">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button id="cancelarboton" fx:id="cancelarboton" layoutX="485.0" layoutY="152.0" mnemonicParsing="false" onAction="#cancelar" prefHeight="48.0" prefWidth="115.0" text="Cancelar" textFill="RED">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
